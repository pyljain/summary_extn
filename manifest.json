{
    "manifest_version": 3,
    "name": "Page Summarizer",
    "version": "1.0",
    "description": "Summarizes the content of the current webpage.",
    "permissions": [
      "scripting",
      "activeTab",
      "storage"
    ],
    "host_permissions": ["http://localhost:9001/*"],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Summarize Page",
        "default_icon": "ce.png"
    }
  }